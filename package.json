{
  "name": "@strg/behave-tracking-client-lib",
  "version": "5.0.3",
  "description": "STRG.BeHave - Tracking Client Library",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "require": "./lib/index.js",
      "types": "./lib/index.d.ts"
    },
    "./src/*": "./src/*"
  },
  "scripts": {
    "start": "npm run test && npm run build",
    "build": "tsc",
    "test": "jest",
    "coverage": "npm run test",
    "coverage:dev": "npm run test && http-server ./coverage/lcov-report/ -p 8888 -o",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "npm run lint && npm run type-check && npm run build && npm test"
  },
  "keywords": [
    "BeHave",
    "STRG.BeHave",
    "tracking",
    "websocket"
  ],
  "author": "STRG.",
  "contributors": [
    "Ralf Traunsteiner <ralf@strg.at>",
    "Nils Müller <nils.mueller@strg.at>"
  ],
  "maintainers": [
    "Nils Müller <nils.mueller@strg.at>"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/strg-at/behave.tracking-client-lib.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "eslint-plugin-jest": "^29.0.0",
    "http-server": "14.1.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^26.1.0",
    "prettier": "3.5.3",
    "ts-jest": "^29.3.2",
    "typescript": "^5.9.2"
  },
  "bugs": {
    "url": "https://github.com/strg-at/behave.tracking-client-lib/issues"
  },
  "homepage": "https://github.com/strg-at/behave.tracking-client-lib#readme",
  "dependencies": {
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.8"
  },
  "files": [
    "lib",
    "src",
    "README.md",
    "LICENSE"
  ]
}
